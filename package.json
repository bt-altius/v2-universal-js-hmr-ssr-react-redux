{
  "name": "univeral-hmr-ssr-react-redux-dokku",
  "version": "1.0.0",
  "description": "Univeral JS application utilizing Hot Module Reloading, Server Side Rendering, Code Splitting with React, Redux, React Router 4 and a heroku like deploy setup with Dokku.",
  "main": "index.js",
  "scripts": {
    "development": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/client.babel.js"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Alexander J Ray <alexjamesray@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.9.6",
    "autoprefixer": "^6.7.7",
    "babel-loader": "7.1.x",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "react-hot-loader": "^4.3.5",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0"
  }
}
